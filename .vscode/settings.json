{
    "files.associations": {
        "*.json5": "jsonc"
    },
    "material-icon-theme.folders.associations": {
      // top level
      ".github/workflows": "ci",
      ".renovate": "robot",
      "bootstrap": "seeders",
      "flux": "pipe",
      "talos": "linux",
      // namespaces
      "actions-runner-system": "github",
      "cert-manager": "guard",
      "external-secrets": "secure",
      "flux-system": "pipe",
      "gameserver": "gamemaker",
      "kube-system": "kubernetes",
      "media": "video",
      "monitoring": "event",
      "network": "connection",
      "openebs-system": "base",
      "rook-ceph": "dump",
      "selfhosted": "server",
      "storage": "base",
      "system-upgrade": "update",
      "volsync": "aws"
    },
    "sops.defaults.ageKeyFile": "age.key",
    "yaml.schemas": {
        "Kubernetes": "./kubernetes/*.yaml"
    },
    "vs-kubernetes": {
        "vs-kubernetes.kubeconfig": "./kubeconfig",
        "vs-kubernetes.knownKubeconfigs": [
          "./kubeconfig"
      ]
    }
}
